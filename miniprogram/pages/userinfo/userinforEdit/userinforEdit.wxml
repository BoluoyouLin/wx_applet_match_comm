<view class='edit-all'>
  <view class='avatar' bindtap='changeAvatar'>
    <image src='{{avatar}}'></image>
  </view>
  <view class="weui-cells weui-cells_after-title">
    <view url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active" bindtap='changeBackgroundImage'>
      <view class="weui-cell__bd">背景墙</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </view>
    <navigator url="../changeNickname/changeNickname" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">昵称</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{nickName}}</view>
    </navigator>
    <navigator url="../changeResume/changeResume" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">个人简介</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{resume}}</view>
    </navigator>
    <navigator url="../changeResume/changeResume" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">标签</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{label}}</view>
    </navigator>
  </view>

</view>